{
  "name": "rulebuilders",
  "displayName": "ruleBuilds",
  "description": "鉴释 ruleBuilders",
  "publisher": "jianshi",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:rulebuilders.helloWorld",
    "onCommand:rulebuilders.fsmHtml-java",
    "onCommand:rulebuilders.fsmHtml-c"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rulebuilders.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "rulebuilders.fsmHtml-java",
        "title": "fsmHtml-java"
      },
      {
        "command": "rulebuilders.fsmHtml-c",
        "title": "fsmHtml-c"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "rulebuilders.fsmHtml-java",
          "group": "1_modification",
          "when": "editorLangId == java"
        },
        {
          "command": "rulebuilders.fsmHtml-c",
          "group": "1_modification",
          "when": "editorLangId == c"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  }
}
